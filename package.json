{
  "name": "@lls/react-light-calendar",
  "version": "2.2.0",
  "description": "ğŸ—“ A lightweight calendar for React (~3 kb)",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "build": "npm run clear:dist && $(npm bin)/rollup -c",
    "build-storybook": "build-storybook",
    "size": "npm run build && $(npm bin)/size-limit",
    "clear:dist": "rm -rf dist",
    "test": "$(npm bin)/jest",
    "lint": "$(npm bin)/eslint src",
    "deploy-storybook": "storybook-to-ghpages",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run size",
    "postpublish": "npm run deploy-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lelivrescolaire/react-light-calendar.git"
  },
  "keywords": [
    "react",
    "calendar",
    "light",
    "lightweight",
    "date",
    "time",
    "datetime",
    "timezone"
  ],
  "author": "Arnaud CETOUTE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lelivrescolaire/react-light-calendar/issues"
  },
  "homepage": "https://github.com/lelivrescolaire/react-light-calendar#readme",
  "dependencies": {
    "lodash.times": "4.3.2",
    "prop-types": "15.7.2",
    "timestamp-utils": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@size-limit/preset-small-lib": "^4.5.0",
    "@storybook/react": "5.1.10",
    "@storybook/storybook-deployer": "2.8.1",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chai": "4.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.3.0",
    "eslint-plugin-react": "7.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-date-mock": "1.0.7",
    "jest-enzyme": "7.1.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "rollup": "1.19.4",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-uglify": "6.0.2",
    "sinon": "7.4.1",
    "size-limit": "4.5.0"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "browserslist": [
    "safari 8",
    "ie 10"
  ],
  "size-limit": [
    {
      "limit": "3.5 KB",
      "path": "dist/index.js",
      "webpack": false
    },
    {
      "limit": "1 KB",
      "path": "dist/index.css",
      "webpack": false
    }
  ]
}
